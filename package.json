{
  "name": "travel-app",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "setup": "yarn && yarn rename && yarn start",
    "clean": "rm -rf node_modules .expo .expo-shared",
    "start:clean": "rm -rf .expo .expo-shared && npx expo start -c",
    "reinstall": "yarn run clean && yarn install",
    "reset": "yarn run clean && yarn cache clean && yarn install",
    "postinstall": "node scripts/patch-safe-area.js && yarn rename",
    "rename": "node scripts/rename.js"
  },
  "dependencies": {
    "expo": "^54.0.32",
    "expo-status-bar": "~3.0.9",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.2"
  },
  "private": true
}